	# Trigger Parameters
	Eval first () ? offset_hitpoint = -2.4*hitpoint + 0.4 + Target_sOffset
	Eval first () ? offset_mass = 1700
	Eval first () ? ttc = 2.59
	Eval first () ? d_tgt = ttc*v_tgt
	Eval first () ? target_sRoadInit = d_tgt_max - d_tgt - ego_wid/2 - offset_hitpoint + offset_mass
	" "
	# Trigger Logic
	Eval TriggerCond = Car.v > 0.01
	Eval first (TriggerCond) ? Traffic.T00.sRoad = target_sRoadInit
	Eval first (TriggerCond) ? Traffic.T00.LongVel = v_tgt
	"  "
	#Resetting
	Eval first (cycleCounter) ? Traffic.T00.Long.StepIdx = 1
	Eval first (TriggerCond) ?  ++cycleCounter
