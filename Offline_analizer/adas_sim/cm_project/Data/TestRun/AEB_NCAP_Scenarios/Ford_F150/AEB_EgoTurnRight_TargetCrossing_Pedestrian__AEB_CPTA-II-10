DrivMan.Man.0.LongStep.0.Cmds:
	#Parameters vehicle
	Eval first () ? ego_len = 4.67
	Eval first () ? d_ego = 114.7 - ego_len
	Eval first () ? v_ego_kph = ($v_ego_kph=32)
	Eval first () ? v_ego = v_ego_kph/3.6
	Eval first () ? hitpoint = ($hitpoint=0.5)
	""
	# Offset (applicable to way pedestrian has to go)
	Eval first () ? offset = 0.3294*ego_len - 0.7908
	""
	#Parameters pedestrian (traffic object)
	Eval first () ? d_tgt = 9.8 - offset
	Eval first () ? v_tgt_kph = ($v_tgt_kph=5)
	Eval first () ? v_tgt = v_tgt_kph/3.6
	Eval t_tgt = d_tgt/v_tgt
	""
	#Trigger Condition
	Eval d_trigger = d_ego - t_tgt*v_ego
	""
	#Trigger Logic
	Eval TriggerCond = Vhcl.Distance >= d_trigger
	Eval first (TriggerCond) ? Traffic.Obj00.LongVel = v_tgt
	""
	# Resetting
	Eval first (cycleCounter) ? Traffic.Obj00.Long.StepIdx = 1
	Eval first (TriggerCond) ? ++cycleCounter
