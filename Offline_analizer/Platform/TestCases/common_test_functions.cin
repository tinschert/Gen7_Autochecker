long WaitForSysVarInRange(
  sysvarFloat * sysvarF,
  double lower_limit,
  double upper_limit,
  dword timeout_in_ms
)
{
  long result;
  
  if(lower_limit <= upper_limit)
  {
    result = TestWaitForSignalInRange(sysvarF, lower_limit, upper_limit, timeout_in_ms);
    
    if(result == 0)
    {
      TestStepFail (0, "", "Sysvar %s::%s did not arrive within expected value range : [%f .. %f]. Last received: %f !", sysvarF.`namespace, sysvarF.name, lower_limit, upper_limit, @sysvarF);
    }
    else if(result == 1)
    {
      TestStepPass (0, "", "Sysvar %s::%s arrived within expected value range : [%f .. %f]. Received : %f", sysvarF.`namespace, sysvarF.name, lower_limit, upper_limit, @sysvarF);
    }
    else
    {
      TestStepFail (0, "", "General error !");
    }
  }
  else
  {
    TestStepFail (0, "", "Range error: upper_limit has to be greater than lower_limit !");
  }
  
  return result;
}

long WaitForSysVarIntInRange(
  sysvarLongLong * sysvarI,
  int64 lower_limit,
  int64 upper_limit,
  dword timeout_in_ms
)
{
  long result;
  
  if(lower_limit <= upper_limit)
  {
    result = TestWaitForSignalInRange(sysvarI, lower_limit, upper_limit, timeout_in_ms);
    
    if(result == 0)
    {
      if(lower_limit != upper_limit)
      {
        TestStepFail (0, "", "Sysvar %s::%s did not arrive within expected value range : [%d .. %d]. Last received: %d !", sysvarI.`namespace, sysvarI.name, lower_limit, upper_limit, @sysvarI);
      }
      else
      {
        TestStepFail (0, "", "Sysvar %s::%s did not arrive within expected value : %d. Last received: %d !", sysvarI.`namespace, sysvarI.name, lower_limit, @sysvarI);
      }
    }
    else if(result == 1)
    {
      if(lower_limit != upper_limit)
      {
        TestStepPass (0, "", "Sysvar %s::%s arrived within expected value range : [%d .. %d]. Received : %d", sysvarI.`namespace, sysvarI.name, lower_limit, upper_limit, @sysvarI);
      }
      else
      {
        TestStepPass (0, "", "Sysvar %s::%s arrived within expected value : %d. Received : %d", sysvarI.`namespace, sysvarI.name, lower_limit, @sysvarI);
      }
    }
    else
    {
      TestStepFail (0, "", "General error !");
    }
  }
  else
  {
    TestStepFail (0, "", "Range error: upper_limit has to be greater than lower_limit !");
  }
  
  return result;
}

dword CheckElapsedTime(
  dword start_time_in_ms,
  dword lower_limit_in_ms,
  dword upper_limit_in_ms
)
{
  dword elapsed_time_in_ms;
  
  elapsed_time_in_ms = timeNow()/100 - start_time_in_ms;
  
  if((lower_limit_in_ms < elapsed_time_in_ms) && (upper_limit_in_ms > elapsed_time_in_ms))
  {
    TestStepPass (0, "", "Elapsed time : %dms. It is in expected range : [%d .. %d]ms", elapsed_time_in_ms, lower_limit_in_ms, upper_limit_in_ms);
  }
  else
  {
    TestStepFail (0, "", "Elapsed time : %dms. It is not in expected range : [%d .. %d]ms", elapsed_time_in_ms, lower_limit_in_ms, upper_limit_in_ms);
  }
  
  return elapsed_time_in_ms;
}

