double tan(double x)
{
  double result;
  result = sin(x) / cos(x);
  return result;
}

double ctan(double x)
{
  double result;
  result = cos(x) / sin(x);
  return result;
}

void GetRWA(double wheelbase_in_mm, double front_axle_length_in_mm, double rwa_fc_in_deg, double &rwa_fl_in_deg, double &rwa_fr_in_deg)
{
  rwa_fl_in_deg = 0;
  rwa_fr_in_deg = 0;
  
  if((wheelbase_in_mm != 0) && (front_axle_length_in_mm != 0) && (rwa_fc_in_deg != 0))
  {
    rwa_fl_in_deg = arctan(1 / (1 / tan(rwa_fc_in_deg/RADIANS_IN_DEGREE) - (front_axle_length_in_mm / 2) / wheelbase_in_mm)) * RADIANS_IN_DEGREE;
    rwa_fr_in_deg = arctan(1 / (1 / tan(rwa_fc_in_deg/RADIANS_IN_DEGREE) + (front_axle_length_in_mm / 2) / wheelbase_in_mm)) * RADIANS_IN_DEGREE;
  }
}

