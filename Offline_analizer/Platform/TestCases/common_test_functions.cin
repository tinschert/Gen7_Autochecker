	target_radar_rr_sim_objdata_obj_length[10] = @RBSData::RR.object10.dLengthN;  
  testWaitForTimeout(100);
}

double Get_YawRate_in_deg_p_s()
{
  double yaw_in_deg_p_s;
  yaw_in_deg_p_s = 0;
  
  if(@hil_vehicle::wheelbase > 0)
  {
    if(sin(@hil_hvm::road_wheel_angle/RADIANS_IN_DEGREE) != 0)
    {
      yaw_in_deg_p_s = ((@hil_hvm::velocity_x/3.6) / (@hil_vehicle::wheelbase*0.001 / sin(@hil_hvm::road_wheel_angle/RADIANS_IN_DEGREE))) * RADIANS_IN_DEGREE;
    }
  }
  
  return yaw_in_deg_p_s;
}