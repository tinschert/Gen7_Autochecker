on preStart {
  long result;
  result=ILControlInit ();//Initialization of CANoe IL,to prevent the IL autostart function.
  switch(result)
  {
    case  0   : write("ILControlInit :: MPC3 :: No error."); break;
    case -1   : write("ILControlInit :: MPC3 :: Momentary state of the IL does not permit this query."); break;
    case -50  : write("ILControlInit :: MPC3 :: Nodelayer is inactive - possibly deactivated in the nodes configuration dialog.");  break;
   }

  MPC3_sim_Disable();
  MPC3_sim_SetCycleTimeOffset();
}

on start {

  ILControlSimulationOn();//Starts the simulation of the IL.
  MPC3_sim_SetCANParameter();
  ILControlStart ();//Cyclical sending starts; setting signals is now possible.
  MPC3_sim_Reset();
}

on stopMeasurement {

  MPC3_sim_DisableMsg();
  MPC3_sim_Disable();
  ILControlStop();//Cyclical sending is stopped; setting signals is now no longer possible.
  ILControlSimulationOff();//Stops the simulation of the IL. After that no other function to control the IL has an effect to the IL.

}

on sysvar_update hil_ctrl::mpc3_sim
{
  MPC3_sim_Reset();
}

on sysvar_update hil_ctrl::variant
{
  MPC3_sim_Reset();
}

on sysvar_update Cus_bus::bus_CAN_MPC3_ON_OFF
{
  MPC3_sim_Reset();
}

void MPC3_sim_Reset()
{

  MPC3_sim_DisableMsg();
  MPC3_sim_Disable();

  if ((@Cus_bus::bus_CAN_MPC3_ON_OFF == 1) && (@hil_ctrl::mpc3_sim == 1))
  {
    switch(@hil_ctrl::variant)
    {
      case a_variant:MPC3_sim_start_a_variant();break;
      case b_variant:MPC3_sim_start_b_variant();break;
    }
    if (@hil_ctrl::hil_mode == @hil_ctrl::hil_mode::Restbus) 
    {
       MPC3_sim_EnableEventMsg();
    }
    else
    {
       MPC3_sim_DisableEventMsg();
    }
  }
}

