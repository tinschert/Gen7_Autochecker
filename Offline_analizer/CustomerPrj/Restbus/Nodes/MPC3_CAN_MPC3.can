  if (@this == 1)
  {
    ILFaultInjectionEnableMsg(CAN_MPC3::Vehicle_CAN_21);
  }
  else{
         ILFaultInjectionDisableMsg(CAN_MPC3::Vehicle_CAN_21);
      }
}

void cfg_Msg_Vehicle_CAN_21(dword aDlc, byte data[])
{
   byte messagebytes[10];
   byte byte_count=0,crc_byte_no =0;
   int loop_index;
   int64 i,k;//For looping
   byte xor,data_byte;
   dword crcTemp;
   byte DataLength;
   dword Msg_id;
   
   DataLength = Msg_Vehicle_CAN_21.DataLength;//Message Data Length
   Msg_id  = Msg_Vehicle_CAN_21.id;//Message ID
   
    if (@CAN_MPC3::Vehicle_CAN_21::Vehicle_CAN_21_Counter_FaultInject != 3)//AC: user defined value
    {

    clearBitsInByteArrayBE(data,12,15,4); //Motorola / Big-endian

    }
    if (@CAN_MPC3::Vehicle_CAN_21::Vehicle_CAN_21_Counter_FaultInject == 0)//AC: No Error, calculate AC
    {
      ++counter_Vehicle_CAN_21_Vehicle_CAN_21_Counter;
      counter_Vehicle_CAN_21_Vehicle_CAN_21_Counter %=15;

      copyBitsToByteArrayBE(counter_Vehicle_CAN_21_Vehicle_CAN_21_Counter,data,12,4); //Motorola / Big-endian
    }
    else if (@CAN_MPC3::Vehicle_CAN_21::Vehicle_CAN_21_Counter_FaultInject == 1)//AC: Error sequence: set AC = 0
    {

      copyBitsToByteArrayBE(0,data,12,4); //Motorola / Big-endian
    }
    else if (@CAN_MPC3::Vehicle_CAN_21::Vehicle_CAN_21_Counter_FaultInject == 2)//AC: Error sequence: Freeze last valid value
    {

      copyBitsToByteArrayBE(counter_Vehicle_CAN_21_Vehicle_CAN_21_Counter,data,12,4); //Motorola / Big-endian
    }
    if (@CAN_MPC3::Vehicle_CAN_21::Vehicle_CAN_21_CRC_FaultInject != 3)//CRC: user defined value
    {

    clearBitsInByteArrayBE(data,0,7,8); //Motorola / Big-endian

    }
   messagebytes[0] = Data_ID_Lookup(Msg_id);
   messagebytes[1] = 0x00;
   crc_byte_no = 7;
   for(loop_index=0;loop_index < 6 ;loop_index++)
       messagebytes[loop_index+2] = data[loop_index+1];
   
    if(@CAN_MPC3::Vehicle_CAN_21::Vehicle_CAN_21_CRC_FaultInject == 0)//CRC: No Error, calculate CRC
    {
      crc_Vehicle_CAN_21_Vehicle_CAN_21_CRC = CRC_Calc_SAEJ1850(crc_byte_no, messagebytes);

      copyBitsToByteArrayBE(crc_Vehicle_CAN_21_Vehicle_CAN_21_CRC,data,0,8); //Motorola / Big-endian
    }
    else if(@CAN_MPC3::Vehicle_CAN_21::Vehicle_CAN_21_CRC_FaultInject == 1)//CRC: Error, set to Zero
    {

      copyBitsToByteArrayBE(0,data,0,8); //Motorola / Big-endian
    }
    else if(@CAN_MPC3::Vehicle_CAN_21::Vehicle_CAN_21_CRC_FaultInject == 2)//CRC: Error, Freeze last valid value
    {

      copyBitsToByteArrayBE(crc_Vehicle_CAN_21_Vehicle_CAN_21_CRC,data,0,8); //Motorola / Big-endian
    }
}


